{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
  "plugins": ["react", "jsx-a11y", "import"],
  "settings": {
    "react": {
      "version": "^16.5.2"
    }
  },
  "rules": {
    "react/display-name": "off"
  },
  "globals": {
    "it": true,
    "expect": true,
    "describe": true,
    "navigator": true,
    "jest": true,
    "test": true
  }
}
